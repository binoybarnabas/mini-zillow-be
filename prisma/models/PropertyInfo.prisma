model PropertyInfo {
  id           String   @id
  images       Blob[]  // One-to-many relation
  price        String
  beds         Int
  baths        Int
  sqft         Int
  address      String
  realtor      String
  description  String
  listingType  Int
  createdAt DateTime @default(now())
  createdBy    Int       // Foreign key
  user      User      @relation(fields: [createdBy], references: [id])
}